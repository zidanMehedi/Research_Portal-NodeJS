		<div class="container-login100" style="width:900px;margin-left: 100px">
      
			<div class="wrap-login101" class="text-center">
				<div class="login100-form-title" style="background-image: url(/images/unnamed.jpg);">
					<span class="login100-form-title-1">
						Allocate External
					</span>
				</div>

				<form method="POST" class="login101-form validate-form">
          			<div class="wrap-input100 validate-input m-b-26" data-validate="Group number is required">
            			<select class="input100" name="group_id">
              				<option value="">Select Group</option>
              			<% for(var i=0; i < grouplist.length; i++){ %>
              				<option value="<%= grouplist[i].group_id%>"><%= grouplist[i].group_id%></option>
              			<% } %>
            			</select>
            			<span class="focus-input100"></span>
            			<%if(error.group_id){%>
              				<b style="color: red;"><%= error.group_id.msg%></b>
            			<%}%>
          			</div>

					<div class="wrap-input100 validate-input m-b-18" data-validate = "External name is required">
						<select class="input100" name="external">
              				<option value="">Select External</option>
              			<% for(var i=0; i < teacherlist.length; i++){ %>
              				<option value="<%= teacherlist[i].faculty_fname%> <%= teacherlist[i].faculty_lname%>"><%= teacherlist[i].faculty_fname%> <%= teacherlist[i].faculty_lname%></option>
              			<% } %>
           				</select>
						<span class="focus-input100"></span>
						<%if(error.external){%>
              				<b style="color: red;"><%= error.external.msg%></b>
            			<%}%>
					</div>

					<div id="login100-form-Btn">
						<button type="submit" class="login101-form-btn" style="margin-right: 50%;">
						Allocate External
						</button>
					</div>
				</form>
			</div>
